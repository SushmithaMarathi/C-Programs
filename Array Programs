#include <stdio.h>
int main()
{
int regno;
char pname[15], dob[10];
long int pnum;
printf("Enter reg no \n");
scanf ("%d", &regno);
printf("Enter Parent name \n");
scanf("%s",pname);
printf("Enter date of birth \n");
scanf("%s", &dob);
printf("Enter Phone number \n");
scanf("%ld", &pnum);
printf("Reg No is : %d\n", regno);
printf("Parents name is : %s\n",pname);
printf(" DOB is: %s\n", dob);
printf("mobile number is: %ld\n",pnum) ;
return 0;
}
