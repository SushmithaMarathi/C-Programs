//Unary Operator
Program:
#include <stdio.h>
void main()
{
int a=10;
printf("%d\n",++a);//11
printf("%d\n",a++);//11
printf("%d\n",++a);//13
printf("%d\n",a++);//13
printf("%d\n",a++);//14 ++a;
printf("%d\n",a++);//16 a++;
printf("%d\n",a);//18
}
Output:
11
11
13
13
14
15
16
---------------
Program:
#include<stdio.h>
int main()
{
int a=10;
printf("%d\n",--a);//9
printf("%d\n",a--);//9
printf("%d\n",--a);//7
printf("%d\n",a--);//7
printf("%d\n",a--);//6
//--Ð°;
printf("%d\n",a--);//4 printf("%d\n",a);//2
a--;
return 0;
}
Output:
9
9
7
7
6
5
--------------
//Binary Operator
//Arithmetic Operator
Program:
#include<stdio.h>
void main()
{
int a,b;
printf("enter the value for a,b\n");
scanf("%d %d",&a,&b);
printf("sum is %d\n",a+b);
printf("diff is %d\n",a-b);
printf("product is %d\n",a*b);
printf("quo is %d\n",a/b);
printf("rem is %d\n",a%b);
}
Output:
enter the value for a,b
23
13
sum is 36
diff is 10
product is 299
quo is 1
rem is 10
-----------------
//Logical Operator
Program:
#include<stdio.h>
void main()
{
int a=2,b=7;
int x=(++a && b++);
printf("a=%d b=%d x=%d\n",a,b,x);//3 8 1
}
Output:
a=3 b=8 x=1
-------------
